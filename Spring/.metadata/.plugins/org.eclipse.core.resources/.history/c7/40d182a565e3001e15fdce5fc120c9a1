	super();
	this.name = name;
	Age = age;
	this.doctor = doctor;
	this.phamarcy = phamarcy;
}
 public void details()
 {
	 System.out.println("hey"+name+"welcome to java");
	 System.out.println("hey"+Age+"welcome to java");
	 doctor.Doctor();
	 phamarcy.phamarcyInfo();
 }
 


}
 

