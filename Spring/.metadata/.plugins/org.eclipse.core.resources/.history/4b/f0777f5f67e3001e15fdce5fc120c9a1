package springex;

public class patient {
 private String name;
 private int  Age;
private  Doctor doctor;
 private Phamarcy phamarcy;
public patient(String name, int age, Doctor doctor, Phamarcy phamarcy) {
	super();
	this.name = name;
	Age = age;
	this.doctor = doctor;
	this.phamarcy = phamarcy;
}
 public void details()
 {
	 doctor.doctorInfo();
	 phamarcy.phamarcyInfo();
 }
 


}
 

